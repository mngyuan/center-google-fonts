{
	"manifest_version": 2,
	"name": "Center Google Fonts",
	"version": "0.1",
	"description": "Centers the google font preview page.",
	"content_scripts": [
		{
			"matches": ["*://www.google.com/fonts/specimen/*"],
			"css": ["centergooglefonts.css"]
		}
	],
	"permissions": ["tabs", "*://www.google.com/fonts/specimen/*"]
}
